    "prettier": "1.9.2",
    "serverless-plugin-typescript": "1.1.5",
    "serverless-apigwy-binary": "1.0.0"
  },
  "dependencies": {
    "aws-sdk": "2.176.0",
    "graphql-request": "1.4.1",
